{
  "name": "timekeeper",
  "version": "1.0.0",
  "description": "An app to help keep track of how you spent your time. Created by Daniel Kolgushev",
  "main": "index.js",
  "scripts": {
    "build:css": "postcss client/css-unbuilt/main.css -o client/css/main.css",
    "watch:css": "postcss client/css-unbuilt/main.css -o client/css/main.css -w",
    "build:ts": "tsc -p tsconfig.json",
    "watch:ts": "tsc -p tsconfig.json --watch",
    "watch:all": "concurrently \"npm run watch:ts\" \"npm run watch:css\"",
    "build:all": "npm run build:ts && npm run build:css",
    "dev": "concurrently \"npm run watch:all\" \"nodemon --exec \\\"npm run start:ts:nocomp\\\"\"",
    "start:ts": "npm run build:all && npm run start:ts:nocomp",
    "start:ts:nocomp": "electron .",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "author": "Daniel Kolgushev",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@tailwindcss/typography": "^0.5.9",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "cssnano": "^6.0.1",
    "electron": "^25.1.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^9.1.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@types/react-dom": "^18.2.4",
    "dotenv": "^16.1.4",
    "electron-squirrel-startup": "^1.0.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0"
  }
}
